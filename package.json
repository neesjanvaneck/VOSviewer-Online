{
  "name": "vosviewer-online",
  "version": "1.0.0",
  "description": "VOSviewer Online",
  "homepage": "https://app.vosviewer.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/neesjanvaneck/VOSviewer-Online.git"
  },
  "author": "Nees Jan van Eck <ecknjpvan@cwts.leidenuniv.nl> (https://github.com/neesjanvaneck)",
  "license": "MIT",
  "module": "src",
  "scripts": {
    "build": "webpack --progress --colors --verbose --display-error-details --env.config config.json --config webpack.config.babel.js",
    "build:vosviewer": "npm run build",
    "build:dimensions": "webpack --progress --colors --verbose --display-error-details --env.config config-dimensions.json --env.mode=dimensions --config webpack.config.babel.js",
    "build:zeta-alpha": "webpack --progress --colors --verbose --display-error-details --env.config config-zeta-alpha.json --env.mode=zetaalpha --config webpack.config.babel.js",
    "build:rori": "webpack --progress --colors --verbose --display-error-details --env.mode=rori --config webpack.config.babel.js",
    "dev": "webpack-dev-server --hot --port 8600 --colors --watch --env.dev --env.config config.json --config webpack.config.babel.js",
    "dev:vosviewer": "npm run dev",
    "dev:dimensions": "webpack-dev-server --hot --port 8600 --colors --watch --env.dev --env.config config-dimensions.json --env.mode=dimensions --config webpack.config.babel.js",
    "dev:zeta-alpha": "webpack-dev-server --hot --port 8600 --colors --watch --env.dev --env.config config-zeta-alpha.json --env.mode=zetaalpha --config webpack.config.babel.js",
    "dev:rori": "webpack-dev-server --hot --port 8600 --colors --watch --env.dev --env.mode=rori --config webpack.config.babel.js"
  },
  "dependencies": {
    "@emotion/core": "^10.0.23",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "autosuggest-highlight": "^3.1.1",
    "d3": "^6.7.0",
    "decode-uri-component": "^0.2.0",
    "dompurify": "^2.3.0",
    "emotion": "^10.0.23",
    "java-random": "^0.4.0",
    "leven-sort": "^3.0.0",
    "lodash": "^4.17.21",
    "material-ui-color": "^1.2.0",
    "material-ui-popup-state": "^1.8.4",
    "mobx": "^4.9.3",
    "mobx-react-lite": "^1.4.1",
    "papaparse": "^5.3.1",
    "qrcode.react": "^1.0.1",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "^5.0.1",
    "use-debounce": "0.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@hot-loader/react-dom": "^17.0.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.23",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-emotion": "^10.0.23",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^4.5.1",
    "raw-loader": "^1.0.0",
